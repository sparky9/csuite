      "targets": [
        {
          "exemplar": false,
          "expr": "sum(rate(http_server_requests_seconds_count{service=\"ocsuite-api\", status=~\"5..\", http_route!\"\"}[5m])) / sum(rate(http_server_requests_seconds_count{service=\"ocsuite-api\", http_route!\"\"}[5m]))",
          "format": "time_series",
          "instant": false,
          "legendFormat": "5xx error ratio",
          "refId": "A"
        }
      ],
