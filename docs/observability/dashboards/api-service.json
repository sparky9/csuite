        "tooltip": {
          "mode": "single"
        }
      },
      "targets": [
        {
          "expr": "sum by (http_route) (rate(ocsuite_http_requests_total{service=\"ocsuite-api\"}[5m]))",
          "legendFormat": "{{http_route}}",
          "refId": "A"
        }
      ],
